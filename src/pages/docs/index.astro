---
const target = '/docs/en/';
Astro.redirect(target);
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="refresh" content={`0; url=${target}`} />
    <link rel="canonical" href={target} />
    <title>Redirecting…</title>
  </head>
  <body>
    <p>Redirecting to <a href={target}>English docs</a>…</p>
  </body>
</html>

